   <div class="text-left">
      <h3>ジャンル一覧.追加</h3>


      <%= form_for @genre,url: admin_genres_path do |f| %>

      <div class="form-group">
      <label for="genre_title">ジャンル名</label>
      <h4><%= f.text_field :name %></h4>
      </div>

      <div class="form-group">
      <%= f.submit '新規登録',class: "btn btn-success" %>
      </div>
      <% end %>


     <tabel class="table">
     <thead>
       <tr>
         <th>ジャンル一覧</th>
       </tr>
     </thead>

     <tbody>
     <% @genres.each do |genre| %>
     <tr>
     <td><%= genre.name %></td>
     </tr>
     <% end %>
     </tbody>
     </tabel>


 </div>